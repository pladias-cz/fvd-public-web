{
	"type": "project",
	"license": ["MIT"],
	"require": {
		"php": ">= 8.4",
    "ext-imagick": "*",
    "ext-intl": "*",
    "ext-exif": "*",
		"nette/application": "^3.2.3",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/di": "^3.2",
		"nette/http": "^3.3",
		"nette/robot-loader": "^4.0",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10",
    "contributte/console": "^v0.9",
    "contributte/console-extra": "^v0.7",
    "nettrine/orm": "^0.8",
    "nettrine/dbal": "^v0.8.2",
    "nettrine/cache": "^0.3",
    "nettrine/migrations": "^v0.9"
  },
	"require-dev": {
		"symfony/thanks": "^1"
	},
	"autoload": {
		"psr-4": {
			"App\\": "App"
		}
	},
	"minimum-stability": "stable",
	"config": {
    "platform": {
      "php": "8.4",
      "ext-imagick": "3",
      "ext-intl": "3",
      "ext-exif": "3"
    },
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
