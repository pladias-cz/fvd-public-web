{
	"type": "project",
	"license": ["MIT"],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/pladias-cz/doctrine-mappings"
    }
  ],
	"require": {
		"php": ">= 8.5",
    "ext-imagick": "*",
    "ext-intl": "*",
    "ext-exif": "*",
		"nette/application": "^3",
		"nette/bootstrap": "^3",
		"nette/caching": "^3",
		"nette/di": "^3",
		"nette/http": "^3",
		"nette/robot-loader": "^4",
    "nette/security": "^3",
		"nette/utils": "^4",
		"latte/latte": "^3",
		"tracy/tracy": "^2",
    "contributte/console": "^0",
    "contributte/console-extra": "^v0",
    "contributte/translation": "^2",
    "phpoffice/phpspreadsheet": "^5",
    "pladias-cz/doctrine-mappings": "@dev",
    "symfony/var-exporter": "^7",
    "68publishers/asset": "^3",
    "68publishers/webpack-encore-bundle": "^3"
  },
	"require-dev": {
		"symfony/thanks": "^1"
	},
	"autoload": {
		"psr-4": {
			"App\\": "App"
		}
	},
	"minimum-stability": "stable",
	"config": {
    "platform": {
      "php": "8.5",
      "ext-imagick": "3",
      "ext-intl": "3",
      "ext-exif": "3",
      "ext-gd": "7"
    },
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
